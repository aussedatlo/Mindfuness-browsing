{
  "name": "chrome_extension",
  "version": "1.0.0",
  "description": "We are all driven by our habits, good ones and unfortunatly bad ones. For me, some are so ingrained that I am aware of them when it's too late:\r When bored on my computer, I will go on YouTube, Reddit or any trending social media for hours, to satisfy my boredom.\r What if I could alert my conscious self that my unconscious self is accessing a problematic website?",
  "main": "index.js",
  "scripts": {
    "prettier:check": "npx prettier --check ./",
    "prettier:write": "npx prettier --write ./",
    "start": "npx react-scripts start",
    "build:popup": "INLINE_RUNTIME_CHUNK=false npx react-scripts build",
    "build:scripts": "npx tsc --p tsconfig.scripts.json",
    "build": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syrald/chrome_extension.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/syrald/chrome_extension/issues"
  },
  "homepage": "/",
  "dependencies": {
    "@types/chrome": "^0.0.236",
    "@types/node": "^16.18.32",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
